function theme_open_subject_node_form($variables) {
    drupal_add_css(drupal_get_path('theme', 'omega').'/addhoc/form-profile.css');
    drupal_add_css(drupal_get_path('theme', 'omega').'/addhoc/sc-form-profile.css');
    drupal_add_css(drupal_get_path('theme', 'omega').'/addhoc/960_12_col.css');

    $form = $variables['form'];

    $output  = '';

    $output .= '<div class="profile-form-wrapper clearfix">

                    <div class="label-title">'.t("เปิดรายวิชา").'</div>
                    <div class="">'.drupal_render($form['field_subject_ref']).'</div>
                    <div class="">'.drupal_render($form['field_curriculum_ref']).'</div>
                    <div class="">'.drupal_render($form['field_teacher_ref']).'</div>
                    <div class="">'.drupal_render($form['field_semester']).'</div>
                    <div class="">'.drupal_render($form['field_school_year']).'</div>
                    <div class="">'.drupal_render($form['field_class']).'</div>
                    <div class="">'.drupal_render($form['field_level']).'</div>
                    <div class="">'.drupal_render($form['field_study_locations']).'</div>
                    <div class="clearfix"></div>

              </div>';

    $output .= '<div class="save-botton">
                    '.drupal_render($form["actions"]["submit"]).'
                    '.drupal_render($form["actions"]["preview"]).'
                    '.drupal_render($form["actions"]["delete"]).'
                </div>';

    $output .= '
                <div class="hidden">'.drupal_render($form['field_updata_time_open_subject']).'</div>
                <div class="clearfix"></div>
            ';

    $output .= drupal_render_children($form);

    return $output;
}

