/***********
* function *
***********/
function display_form_subjects_group_page_form($nid){
    $output = '';

    $node = node_load($nid);

    if ($node->type == 'subjects_category') {
        /*load curriculum content*/
        $curriculum = node_load($node->field_curriculum_ref['und'][0]['nid']);
    }
    else if ($node->type == 'subjects_group') {
        /*load subjects_category content*/
        $subjects_category = node_load($node->field_subjects_category_ref['und'][0]['nid']);

        /*load curriculum content*/
        $curriculum = node_load($subjects_category->field_curriculum_ref['und'][0]['nid']);
    }
    else if ($node->type == 'sub_subjects_group') {
        /*load subjects_group content*/
        $subjects_group = node_load($node->field_subjects_group_ref['und'][0]['nid']);

        /*load subjects_category content*/
        $subjects_category = node_load($subjects_group->field_subjects_category_ref['und'][0]['nid']);

        /*load curriculum content*/
        $curriculum = node_load($subjects_category->field_curriculum_ref['und'][0]['nid']);
    }


    //$output .= '<div class=link-back>'.l(t("Back"),'node/'.$curriculum->nid).'</div>';

    $subjects_detail = '';
    if (isset($node->field_subjects_detail['und'])) {
        $subjects_detail = $node->field_subjects_detail['und'][0]['value'];

        $output .= '<div class="subjects-detail">
                '.$subjects_detail.'
                </div>';
    }


    $header = array(
        array('class'=>'header-1', 'data'=>t("ลำดับ")),
        array('class'=>'header-1', 'data'=>t("รหัสวิชา")),
        array('class'=>'header-3', 'data'=>t("ชื่อรายวิชา")),
        array('class'=>'header-1', 'data'=>t("หน่วยกิต")),
    );

    $rows = array();

    $query2 = db_select('field_data_field_all_subject_group_ref', 'k');
    $result2 = $query2
                  ->fields('k', array('entity_id'))
                  ->condition('k.field_all_subject_group_ref_nid', $nid, '=')
                  ->orderBy('k.entity_id', 'ASC')
                  ->execute();

    $count = 1;
    $check_row = false;
    while ($entity_id2 = $result2->fetchColumn()) {
        $check_row = true;
        $subject_property = node_load($entity_id2);

        /*load taxonomy*/
        $subject_property_name = taxonomy_term_load($subject_property->field_subject_property_name['und'][0]['tid']);

        $rows[] = array(
            array('class'=>'header-2', 'data'=>$subject_property_name->name, 'colspan'=>3),
            array('class'=>'header-2', 'data'=>display_form_concerned_subjects_edit_link($subject_property->nid)),
        );

        $query3 = db_select('field_data_field_subjects_ref', 'i');
        $result3 = $query3
                      ->fields('i', array('field_subjects_ref_nid'))
                      ->condition('i.entity_id', $subject_property->nid, '=')
                      ->orderBy('i.entity_id', 'ASC')
                      ->execute();


        $credit = null;
        while ($entity_id3 = $result3->fetchColumn()) {
            $subjects_group = node_load($entity_id3);

            $subjects_group_link = $subjects_group->field_subjects_name_th['und'][0]['value'];

            if (isset($subjects_group->field_subjects_time['und'][0]['value'])) {
                $credit = $subjects_group->field_subjects_credit['und'][0]['value'].' ('
                            .$subjects_group->field_subjects_time['und'][0]['value'].')';
            }
            else {
                $credit = $subjects_group->field_subjects_credit['und'][0]['value'].' ('
                        .$subjects_group->field_subjects_theory['und'][0]['value'].'-
                        '.$subjects_group->field_subjects_practice['und'][0]['value'].'-
                        '.$subjects_group->field_subjects_research['und'][0]['value'].')';
            }

            $destination = drupal_get_destination();
            //$de = 'ffff';

            $rows[] = array(
                array('class'=>'row-1', 'data'=>$count),
                array('class'=>'row-1', 'data'=>$subjects_group->field_subjects_id['und'][0]['value']),
                array('class'=>'', 'data'=>l(t($subjects_group_link), 'node/'.$subjects_group->nid
                                            , array('query' => array('des' => $curriculum->nid, 'des2' => $destination)))),
                array('class'=>'row-1', 'data'=>$credit),
            );

            $count++;

        }
    }


    $query = db_select('field_data_field_subjects_ref', 'i');
    $result = $query
                  ->fields('i', array('field_subjects_ref_nid'))
                  ->condition('i.entity_id', $nid, '=')
                  ->orderBy('i.entity_id', 'ASC')
                  ->execute();

    $count = 1;
    $credit = null;
    while ($entity_id = $result->fetchColumn()) {
        $check_row = true;

        $subjects_group = node_load($entity_id);

        $subjects_group_link = $subjects_group->field_subjects_name_th['und'][0]['value'];

        $subject_theory = '';
        if (isset($subjects_group->field_subjects_theory['und'])) {
            $subject_theory = $subjects_group->field_subjects_theory['und'][0]['value'];
        }
        $subject_practice = '';
        if (isset($subjects_group->field_subjects_practice['und'])) {
            $subject_practice = $subjects_group->field_subjects_practice['und'][0]['value'];
        }
        $subject_research = '';
        if (isset($subjects_group->field_subjects_research['und'])) {
            $subject_research = $subjects_group->field_subjects_research['und'][0]['value'];
        }

        if (isset($subjects_group->field_subjects_time['und'][0]['value'])) {
            $credit = $subjects_group->field_subjects_credit['und'][0]['value'].' ('
                        .$subjects_group->field_subjects_time['und'][0]['value'].')';
        }
        else {
            $credit = $subjects_group->field_subjects_credit['und'][0]['value'].' ('
                    .$subject_theory.' - '.$subject_practice.' - '.$subject_research.')';
        }

        $destination = drupal_get_destination();

        $rows[] = array(
            array('class'=>'row-1', 'data'=>$count),
            array('class'=>'row-1', 'data'=>$subjects_group->field_subjects_id['und'][0]['value']),
            array('class'=>'', 'data'=>l(t($subjects_group_link), 'node/'.$subjects_group->nid
                                            , array('query' => array('des' => $curriculum->nid, 'des2' => $destination)))),
            array('class'=>'row-1', 'data'=>$credit),
        );

        $count++;
    }

    if ($check_row) {
        $output .= '<div class="subjects-group-page">
                    '.theme("table", array("header"=>$header, "rows"=>$rows)).'
                </div>';
    }

    return $output;
}
